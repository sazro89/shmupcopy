--[[pod_format="raw",created="2024-11-08 00:54:54",modified="2024-11-08 22:40:40",revision=143]]
function draw_table()
	cls(2)
	spr(10, 10, 10)
	for i = 1, #data do
		for j = 1, #data[i] do
			bgprint(data[i][j],-4+(7*j),-7+(12*i),7)
		end
	end
end
