include("blob.lua")
include("explode.lua")
include("spark.lua")

function _init()
	parts = {}
	slowmo = false

	t = 0
end

function _draw()
	cls(12)
	for p in all(parts) do
		if p.wait == nil then
			p.draw(p)
		end
	end
	--	print(myblb.r, 236, 250, 7)
	--	print(dither_mask(1.1/16), 2, 2, 7)
