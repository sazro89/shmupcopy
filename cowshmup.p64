function doenemies()
	for e in all(enemies) do
		if e.brain == 1 then
			if e.age < 12 then
				-- fly down
				e.sy = 1.5
			elseif e.age < 120 then
				if e.age == 60 then
					add(buls, {
						x = e.x,
						y = e.y,
						sx = 0,
						sy = 2,
						sani = { 21, 22 },
						si = 1,
					})
				end
				-- stay
				e.sy = max(0, e.sy - 0.03)
			else
				-- fly up
				e.sy = e.sy - 0.04
				if e.y < -16 then
					del(enemies, e)
				end
			end
		end
		e.age = e.age + 1
		e.x = e.x + e.sx
		e.y = e.y + e.sy
		e.si = e.si + 0.15
		if flr(e.si) > #e.sani then
			e.si = 1
		end
	end
end

function doshots(arr)
	for s in all(arr) do
		s.x = s.x + s.sx
		s.y = s.y + s.sy
		s.si = s.si + 0.5

		if flr(s.si) > #s.sani then
			s.si = 1
		end
		if s.y < -16 or s.y > 280 then
			del(arr, s)
		end
	end
end

function shoot()
	local shotspd = -12
	shotwait = 2
	add(shots, {
		x = px - 5 + banked - xscroll,
		y = py - 4,
		sx = 0,
		sy = shotspd,
