--[[pod_format="raw",created="2024-11-08 00:54:54",modified="2025-02-22 07:44:15",revision=2981]]
function draw_setup()

end

function draw_brain()
	cls(18)
	
--	debug[1] = tostr(overlay)
	
	drawgrid(back_grid, 16)
	drawgrid(front_grid, 6)
	
	circ(pspr.x, pspr.y-scrolly, 4, 11)
	
	if show_trails then
		for t in all(curtrails) do
			pset(t[1], t[2]-scrolly, 9)
		end
	end
	
	for e in all(enemies) do
		drawobj(e)
		if overlay and e == protag then
			local myang = e.ang
			if e.spd < 0 then
				myang = -myang
			end
			
			local ox = sin(myang)
			local oy = cos(myang)
			
			local ox1 = e.x + ox * 10
			local oy1 = e.y + oy * 10
			pset(e.x, e.y-scrolly, 9)
			line(ox1, oy1 - scrolly, ox1 + ox * 8 * abs(e.spd), oy1 - scrolly + oy * 8 * abs(e.spd), 9)
		end
	end	
	
	for m in all(muzz) do
		m.r -= 1
		if m.en and m.r > 0 then
			circfill(m.en.x, m.en.y-scrolly, m.r, 7)
		end
		
		if m.r <= 0 then
			del(muzz, m)
		end
	end
	
	rectfill(0,0-scrolly,101,200-scrolly,1)
	
	line(102,0-scrolly,102,200-scrolly, 6)
	
	draw_menu()
	bgprint("        ", 62, 177-scrolly, 0)
	bgprint("        ", 62, 189-scrolly, 0)
	print("asp: ", 62, 178-scrolly, 24)
	print("adr: ", 62, 190-scrolly, 24)
	if enemies[1] then
		if enemies[1].aspt then
			local aspt = string.format("%.2f", abs(enemies[1].aspt - enemies[1].spd))
			print("asp:" .. aspt, 62, 178-scrolly, 24)
		end
		if enemies[1].adrt then
			local adrt = string.format("%.2f", abs(enemies[1].adrt - enemies[1].ang))
			print("adr:" .. adrt, 62, 190-scrolly, 24)
		end
	end
--	bgprint(mymenu.w, mymenu.x, mymenu.y, _c) --*****
--	print(mymenu.text, mymenu.x, 1+mymenu.y, _c)
--	if myen.aspt then
--	
--	end
--	
--	if myen.adrt then
--
--	end
end

function draw_table()
	cls(1)
	
	draw_menu()
end

function draw_menu()

	if menu then
		for _i = 1, #menu do
			for _j = 1, #menu[_i] do
				-- change color if cell is currently selected
				local _c = menu[_i][_j].c or 22
				if _i == cury and _j == curx then
					_c = 7
				end
				
				-- make copy of local cell we're working with
				local mymenu = menu[_i][_j]
				
				-- print cell
				bgprint(mymenu.w, mymenu.x, mymenu.y, _c) --*****
				print(mymenu.text, mymenu.x, 1+mymenu.y, _c)
				if _i == cury and _j == curx then
					rect(mymenu.x-2, mymenu.y-2, mymenu.x+(#mymenu.w * 5), mymenu.y+11, _c)
				end
				if _i == execy then
--					rect(mymenu.x-2, mymenu.y-2, mymenu.x+(#mymenu.w * 5), mymenu.y+11, 14)
					line(0,-10 + 13 * execy,0,1 + 13 * execy,14)
				end
			end
		end
	end
end

function drawgrid(grid, col)
	for l in all(grid) do
		line(l+116,1-scrolly-1,l+116,200-scrolly-1,col)
		line(101,200-l-scrolly-1,300,200-l-scrolly-1,col)
	end
end
