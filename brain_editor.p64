function doenemies()
	for e in all(enemies) do
		if e.wait > 0 then
			e.wait -= 1
		elseif e.dist <= 0 then
			dobrain(e)
		end
		
		if e.flw then
			local diff = atan2(pspr.y - e.y,  pspr.x - e.x) - e.ang
			
			if abs(diff) > 0.5 then
				diff -= sgn(diff)
			end
			
			e.ang += mid(-e.flws, diff, e.flws)
			if dist(pspr.x, pspr.y, e.x, e.y) < e.flwd then
				e.flw = false
			end
			
			e.ang = e.ang % 1
		end
		
		if e.aspt then
			e.spd += e.asps
			if abs(e.aspt - e.spd) < abs(e.asps) then
				e.spd = e.aspt
				e.aspt = nil
			end
		end
		
		if e.adrt then
			e.ang += e.adrs
			if abs(e.adrt - e.ang) < abs(e.adrs) then
				e.ang = e.adrt
				e.adrt = nil
			end
		end
		
		e.sx = sin(e.ang) * e.spd
		e.sy = cos(e.ang) * e.spd
		e.dist = max(0, e.dist - abs(e.spd))
		
		e.x = e.x + e.sx
		e.y = e.y + e.sy
		
		e.age = e.age + 1	
		
--		if protag then
		add(newtrails, {e.x, e.y})
--		end

		if not onscreen(e) then
			del(enemies, e)
		end
	end
	

end

function dobrain(e)
	if brain_check(e) == false then return end

	local my_brain = brains[e.brain]
	local exit = false
	
	if e.brain_index < #my_brain then
		local cmd = my_brain[e.brain_index]
		local param1 = my_brain[e.brain_index + 1]
		local param2 = my_brain[e.brain_index + 2]
		
		if cmd == "hed" then
			-- set heading / speed
			e.ang = param1
			e.spd = param2
			e.aspt = nil
			e.flw = false
		elseif cmd == "wai" then
			-- set timer for previous command
			e.wait = param1 
			e.dist = param2
--			exit = true
		elseif cmd == "asp" then
			-- accelerate speed
			e.aspt = param1
			e.asps = param2
		elseif cmd == "adr" then
			-- accelerate direction
			e.adrt = param1
			e.adrs = param2
			e.flw = false
		elseif cmd == "got" then
			-- goto brain / index
			e.brain = param1
			e.brain_index = param2 - 3
		elseif cmd == "lop" then
			-- loop
			e.loop = e.loop and e.loop + 1 or 1
			if e.loop < param1 then
				e.brain_index = param2 - 3
			else
				e.loop = 0
			end
		elseif cmd == "fir" then
			-- fire all lasers!!
			firebul(e, param1, param2)
		elseif cmd == "clo" then
			-- clone enemy with a delay
			for i = 1, param1 do
				local my_clone = copylist(e)
				my_clone.wait += i * param2
				my_clone.brain_index += 3
				add(enemies,my_clone)
			end
		elseif cmd == "flw" then
			-- follow
			e.flw = true
			e.flws = param1
			e.flwd = param2
		else
			return
		end
	end
	-- advance index to next command (always comes in threes)
	e.brain_index += 3
	
--	if exit then return end
	-- makes it so commands will skip straight to wait statements within 1f

--	dobrain(e)
end

function onscreen(obj)
	if obj.x < 84 then return false end
	if obj.y <  -16 then return false end

	if obj.x >  316 then return false end
	if obj.y >  216 then return false end
	
	return true
end

function dogrid(grid, spacing, speed)
	if #grid == 0 then
		for i = -spacing, 226, spacing do
			add(grid, i)
		end
	else
		updategrid(grid, spacing, speed)
	end
--	if not grid[1] then
--		-- initialize grid
--		for i = (spacing * -1), i < 226, spacing do
--			add(grid, i)
--		end
--	else
--		-- update grids
--		updategrid(grid, spacing, speed)
--	end
end

function updategrid(grid, spacing, speed)
	for l = 1, #grid do
		local addline = false
		if #grid == l and grid[l] <= 210 then
			del(grid, grid[1])
			add(grid, 210 + spacing)
		end
		
		grid[l] -= speed
	end
end

function brain_check(e)
	if brains[e.brain] == nil then
		if #msg > 0 then
			msg[1].t = 5
		else
			add(msg,{text="brain #" .. e.brain .. " does not exist!",t=120})
		end
		return false
	end
	
	local my_brain = brains[e.brain]
	
	if e.brain_index < 1 then
		if #msg > 0 then
			msg[1].t = 5
		else
			add(msg,{text="command index < 0!",t=120})
		end
		return false
	elseif e.brain_index < #my_brain then
		local cmd = my_brain[e.brain_index]
		local found = false
		for c in all(cmdlist) do
			if c == cmd then
				found = true
			end
		end
		if found == false then
			if #msg > 0 then
				msg[1].t = 5
			else
				add(msg,{text="command " .. cmd .. " does not exist!",t=120})
			end
			return false
		end
	end
	
	return true
end


function firebul(_en, param1, param2)
	add(muzz,{
		en = _en,
		r = 8,
	})
end

function copylist(org)
	local ret = {}
	for k, v in pairs(org) do
		ret[k] = v
	end
	return ret
end

function dist(x1, y1, x2, y2)
	local dx, dy = x2 - x1, y2 - y1
	return sqrt(dx * dx + dy * dy)
end
