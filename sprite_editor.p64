--[[pod_format="raw",created="2024-11-08 00:55:08",modified="2025-03-03 21:44:25",revision=2625]]
function update_edit()
	refresh_edit()

	curx = (cury == 1 or cury >= 10) and 1 or 2
	wheel_y = ({mouse()})[5]

	--up
	if keyp("up") then
		cury = cury - 1
	end
	--down
	if keyp("down") then
		cury = cury + 1
	end
	cury = (cury-1) % (#menu) + 1
	cury = cury - wheel_y
	cury = mid(1, cury, #menu)

	if cury == 1 then
		if keyp("left") then
			selected_sprite -= 1
		end
		if keyp("right") then
			selected_sprite += 1
		end
		selected_sprite = mid(1, selected_sprite, #data)
	elseif cury > 1 and cury <= 6 then
		local mymenu = menu[cury][curx]
		if keyp("left") then
			data[mymenu.cmdy][mymenu.cmdx] -= 1
			dirty = true
		end
		if keyp("right") then
			data[mymenu.cmdy][mymenu.cmdx] += 1
			dirty = true
		end
	end

	if keyp("x") then
		_drw = draw_list
		_upd = update_list
		curx = 1
		cury = selected_sprite
		scrolly = oldscrolly
		refresh_list()
		return
	end

	if keyp("z") then
		mymenu = menu[cury][curx]
		if mymenu.cmd == "editval" then
			current_page = "edit"
			prev_upd = "update_edit"
			text_bar = spawntextbox()
			_upd = upd_type
			text_bar:set_keyboard_focus(true)
			text_bar:set_text(tostr(data[mymenu.cmdy][mymenu.cmdx]))
			mymenu.data = tonum(mymenu.data)
		elseif mymenu.cmd == "editcol" then
			_upd = upd_type
			current_page = "edit"
			prev_upd = "update_col"
			text_bar = spawntextbox()
			text_bar:set_keyboard_focus(true)
			text_bar:set_text(tostr(meta[selected_sprite][2]))
			mymenu.data = tonum(mymenu.data)
		elseif mymenu.cmd == "sprhead" then
			_upd = upd_type
			current_page = "edit"
			prev_upd = "update_name"
			text_bar = spawntextbox()
			text_bar:set_keyboard_focus(true)
			text_bar:set_text(tostr(meta[selected_sprite][1]))
			dirty = true
		elseif mymenu.cmd == "delspr" then
			deli(data,selected_sprite)
			deli(meta,selected_sprite)
			_drw = draw_list
			_upd = update_list
			curx = 1
			cury = 1
			refresh_list()
			dirty = true
		elseif mymenu.cmd == "copyspr" then
			local newspr = copylist(data[selected_sprite])
			local newmeta = copylist(meta[selected_sprite])
			add(data, newspr)
			add(meta, newmeta)
			_drw = draw_list
			_upd = update_list
			selected_sprite = #data
			curx = 1
			cury = 1
			refresh_list()
			dirty = true
		end
	end

	printh("cury: " .. cury .. ", curx: " .. curx)
	local curmenu = menu[cury][curx]

	if curmenu then
		if curmenu.y + scrolly > (200 - 30) then
			scrolly = scrolly - scrollspeed
		end
		if curmenu.y + scrolly < 22 then
			scrolly = scrolly + scrollspeed
		end
		scrolly = min(0, scrolly)

		camera(0,-scrolly)
	end
end

function update_list()
	refresh_list()

	wheel_y = ({mouse()})[5]

	--up
	if keyp("up") then
		cury = cury - 1
	end
	--down
	if keyp("down") then
		cury = cury + 1
	end
	cury = (cury-1) % (#menu) + 1
	cury = cury - wheel_y
	cury = mid(1, cury, #menu)

	local curmenu = menu[cury][curx]

	if curmenu then
		if curmenu.y + scrolly > (200 - 30) then
			scrolly = scrolly - scrollspeed
		end
		if curmenu.y + scrolly < 22 then
			scrolly = scrolly + scrollspeed
		end
		scrolly = min(0, scrolly)

		camera(0,-scrolly)
	end

	--left and right control zoom between 1-5x
	if keyp("left") then
		magnification -= 1
	end
	if keyp("right") then
		magnification += 1
	end
	magnification = mid(1,magnification,5)

	if keyp("z") then
		local mymenu = menu[cury][curx]
		if mymenu.cmd == "newline" then
			add(data,{0, 0, 0, 0, 0})
			add(meta,{"new",0})
			dirty = true
		elseif mymenu.cmd == "editspr" then
			oldscrolly = scrolly
			selected_sprite = mymenu.cmdy
			_upd = update_edit
			_drw = draw_edit
			cury = 1
			scrolly = 0
			scrollx = 0
		end
	end
end

function update_table()
	refresh_table()

	wheel_y = ({mouse()})[5]

	--up
	if keyp("up") then
		cury = cury - 1
	end
	--down
	if keyp("down") then
		cury = cury + 1
	end
	cury = (cury-1) % (#menu) + 1
	cury = cury - wheel_y
	cury = mid(1, cury, #menu)

	--left
	if keyp("left") then
		curx = curx - 1
	end
	--right
	if keyp("right") then
		curx = curx + 1
	end
	if cury < #menu then
		curx = (curx-2) % (#menu[cury]-1) + 2
	else
		curx = 1
	end

	local curmenu = menu[cury][curx]

	if curmenu then
		if curmenu.y + scrolly > (200 - 30) then
			scrolly = scrolly - scrollspeed
		end
		if curmenu.y + scrolly < 22 then
			scrolly = scrolly + scrollspeed
		end
		scrolly = min(0, scrolly)

		if curmenu.x + scrollx > (400 - 80) then
			scrollx = scrollx - scrollspeed
		end
		if curmenu.x + scrollx < 22 then
			scrollx = scrollx + scrollspeed
		end
		scrollx = min(0, scrollx)

		camera(-scrollx,-scrolly)
	end

	-- selecting a cell
	if keyp("z") then
		mymenu = menu[cury][curx]
		-- editable cell selected
		if mymenu.cmd == "edit" then
			current_page = "edit"
			text_bar = spawntextbox()
			_upd = upd_type
			text_bar:set_keyboard_focus(true)
			text_bar:set_text(tostring(mymenu.text))
			mymenu.data = tonum(mymenu.data)
		-- newline cell selected
		elseif mymenu.cmd == "newline" then
			add(data, { 0 })
			dirty = true
		elseif mymenu.cmd == "newcell" then
			add(data[mymenu.cmdy],0)
			dirty = true
		end
	end
end

function upd_type()
	-- no need to check for input if we're typing
end

function spawntextbox(_i)
	local enter_mode = _i
	local editval
	local text_bar = modify_text_display:attach_text_editor{
		x = mymenu.x+14+scrollx, y = mymenu.y-1+scrolly,
		width = 50,
		height = 12,
		margin_top = 2,
		key_callback = {
			enter = function(enter_mode)
				editval = text_bar:get_text()[1]
				if (editval == "") and prev_upd ~= "update_name" then
					editval = 0
					dirty = true
				end
				removeAllChildren(modify_text_display)
				current_page = nil
				if prev_upd == "update_edit" then
					data[mymenu.cmdy][mymenu.cmdx] = (editval) and editval or 0
					dirty = true
				elseif prev_upd == "update_col" then
					meta[selected_sprite][2] = (tonum(editval) > 1) and tonum(editval) or 0
					dirty = true
				elseif prev_upd == "update_name" then
					meta[selected_sprite][1] = editval
					dirty = true
				end
				_upd = update_edit
			end
		}
	}
	return text_bar
end
